    "docx": "^9.2.0",
    "electron-log": "^5.3.2",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "electron-window-state": "^5.0.3",
    "epub": "^1.3.0",
    "fs-extra": "^11.3.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "officeparser": "^5.1.1",
    "p-queue": "^8.1.0",
